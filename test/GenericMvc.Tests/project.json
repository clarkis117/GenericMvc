{
  "testRunner": "xunit",

  "buildOptions": {
    "warningsAsErrors": false,
    "preserveCompilationContext": true,
    "copyToOutput": "Data\\"
  },

  "dependencies": {
    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-rc2-build10025",
    "NETStandard.Library": "1.6.1",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.0.2",
    "Microsoft.EntityFrameworkCore.InMemory": "1.0.2",
    "GenFu": "1.2.1",
    "Microsoft.DotNet.InternalAbstractions": "1.*",
    "GenericMvc": { "target": "project" },
    "GenericMvc.Test.Library": { "target": "project" }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "define": [ "ASYNCLOCAL" ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}